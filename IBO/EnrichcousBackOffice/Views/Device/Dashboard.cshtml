                                        <tbody>
                                            <tr>
                                                <th>Order</th>
                                                <td>#<b id="order_code">------</b></td>
                                            </tr>
                                            <tr>
                                                <th>Merchant</th>
                                                <td id="bundle_info_merchantname">------</td>
                                            </tr>
                                            <tr>
                                                <th>Argent</th>
                                                <td id="argent">-----</td>
                                            </tr>
                                            <tr>
                                                <th>Progress status</th>
                                                <td id="progress_status">-----</td>
                                            </tr>
                                            <tr>
                                                <td colspan="2">
                                                    <b>Note: </b>
                                                    <p id="bundle_info_info">----------</p>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td colspan="2">
                                                    <b>Devices: </b>
                                                    <table id="bundle_info_code_device" class="table table-hover ">
