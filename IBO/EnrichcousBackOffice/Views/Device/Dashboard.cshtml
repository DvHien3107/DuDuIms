@model IEnumerable<EnrichcousBackOffice.Models.CustomizeModel.OrderPackage_view>
@using EnrichcousBackOffice.Models;
@using EnrichcousBackOffice.Utils.IEnums;
@using Inner.Libs.Helpful;
@{
    ViewBag.Title = "Device dashboard";
    List<O_Orders> list_history = ViewBag.history;
    Dictionary<string, bool> p = ViewBag.p;
    var host = $"{HttpContext.Current.Request.Url.Scheme}://{HttpContext.Current.Request.Url.Authority}";
}

<section class="content-header">
    <h1>
        Dashboard
        <small>Deploy</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="/home"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><i class="fa fa-truck"></i> Deployment</li>
        <li class="active">Dashboard</li>
    </ol>
</section>
<section class="content">
    @if (TempData["e"] != null)
    {
        <div class="alert alert-warning alert-dismissible">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
            <span><i class="icon fa fa-warning"></i> @TempData["e"]</span>
        </div>
    }
    else if (TempData["s"] != null)
    {
        <div class="alert alert-success alert-dismissible">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
            <span><i class="icon fa fa-check"></i> @TempData["s"]</span>
        </div>
    }
    <div class="row">
        <div class="col-md-9">
            <div class="box box-primary">
                <div class="box-body">
                    <div class="pull-left" id="filter_status">
                        <a href="/device/dashboard?page=preparation" data-status="preparation"
                           class="btn btn-app @if ("preparation".Equals(TempData["Page"])) { @Html.Raw("active") }">
                            <i class="fa fa-eye"></i> Open
                        </a>
                        <a href="/device/dashboard?page=complete" data-status="complete"
                           class="btn btn-app @if ("complete".Equals(TempData["Page"])) { @Html.Raw("active") }">
                            <i class="fa  fa-check"></i> Closed
                        </a>
                        <a href="/device/dashboard?page=all" data-status="all"
                           class="btn btn-app @if ("all".Equals(TempData["Page"]) || string.IsNullOrEmpty(TempData["Page"]?.ToString())) { @Html.Raw("active") }">
                            <i class="fa fa-th-list "></i> All
                        </a>
                    </div>

                    <button type="button" class="btn btn-primary" style="float:right; margin-left: 10px" onclick="GetProcessSetting()" @if (p.Any(k => k.Key.Equals("inventory_process_setting")) != true || p["inventory_process_setting"] != true) { @Html.Raw("disabled") }>
                        <i class="fa fa-cogs"></i> Manage
                    </button>
                    <div class="col-md-12" style="padding:0px;padding-top:10px">
                        @Html.Partial("_Dashboard_load", Model)
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-3">
            <div class="box box-warning">
                <div class="box-header with-border">
                    <h3 class="box-title">Notification</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body" style="height:80vh; overflow: auto">
                    <div id="List_history">
                        @foreach (var item in list_history)
                        {
                            <div id="history_@item.Id" class="col-md-12 history_item" style="border: 1px solid orange; padding:10px; margin-bottom:10px">
                                <div>
                                    <b><a style="color:#337ab7" onclick="show_progress_modal(@item.Id)">Order: #@item.OrdersCode</a></b>
                                    @if (item.BundelStatus == EnrichcousBackOffice.AppLB.UserContent.DEPLOYMENT_PACKAGE_STATUS.Ready.ToString())
                                    {
                                        <i class="pull-right"><span class="label label-primary">@item.BundelStatus</span></i><br />
                                    }
                                    else if (item.BundelStatus == EnrichcousBackOffice.AppLB.UserContent.DEPLOYMENT_PACKAGE_STATUS.Complete.ToString())
                                    {
                                        <i class="pull-right"><span class="label label-success">@item.BundelStatus</span></i><br />
                                    }
                                    else
                                    {
                                        <i class="pull-right"><span class="label label-default">@item.BundelStatus</span></i><br />
                                    }


                                    <label>Merchant:</label> @item.CustomerName<br />
                                    <label style="color:gray">Grand total: <span class="label label-primary">$ @item.GrandTotal</span></label><br />
                                    @{
                                        var lastUpdate = item.UpdatedHistory?.Split('|').Last().Replace("-", "<br/>");
                                        DateTime date = DateTime.MinValue;
                                        string name = "";
                                        if (!string.IsNullOrEmpty(lastUpdate))
                                        {
                                            date = Convert.ToDateTime(lastUpdate.Substring(0, lastUpdate.IndexOf("<br/>")));
                                            name = lastUpdate.Substring(lastUpdate.IndexOf("<br/>"));
                                        }
                                    }
                                    <i>
                                        <span id="localtime-@item.Id">
                                            <script>convertLocalTime(@item.Id, "@date")</script>
                                        </span>
                                        @Html.Raw(name)
                                    </i>
                                </div>

                            </div>
                        }
                    </div>
                    <button type="button" class="btn btn-block btn-flat btn-success" id="add_history" onclick="load_more_history()" style="@if (!ViewBag.canViewMore) { @Html.Raw("display:none")}">
                        View more...<img id="load_history_img" src="~/Content/ajax-loaders/ajax-loader-1.gif" style="display:none" />
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>



<div id="div_process_setting">
    <!--Append _PartialProcessSetting-->
</div>

<!--invoice detail-->
<div class="modal fade" id="view_invoice" role="dialog">
    <div class="modal-dialog modal-lg">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">View invoice</h4>
            </div>
            <div class="modal-body">
                <div>
                    <div id="invoice_partial_div" style="border:1px solid lightgrey; ">
                        <!--Append _InvoiceDetailPartial-->
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>


<!--packaging-->
<div class="modal fade" id="delivery_bundle" role="dialog">
