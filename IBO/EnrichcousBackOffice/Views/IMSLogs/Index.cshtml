@model IEnumerable<EnrichcousBackOffice.Models.Ts_Task>
@using EnrichcousBackOffice.Models

@{
    Layout = "~/Views/IMSLogs/_IMSLogLayout.cshtml";
    WebDataModel db = new WebDataModel();
    const string LogsPage = "Logs";
    var Page = ViewBag.Page as string;
}

<section class="content">
    <div class="row">

        @if (TempData["e"] != null)
        {
            <div class="alert alert-warning alert-dismissible">
                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                <span><i class="icon fa fa-warning"></i> @TempData["e"]</span>
            </div>
        }
        else if (TempData["s"] != null)
        {
            <div class="alert alert-success alert-dismissible">
                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                <span><i class="icon fa fa-check"></i> @TempData["s"]</span>
            </div>
        }
        <div class="col-md-12">
            <div class="box box-primary">

                <!--<div class="box-header with-border">
                    <h1 style="font-size:17px;margin:0px;">@(Page == LogsPage ? "Logs": "Api Request")</h1>-->
                <!-- /.box-tools -->
                <!--</div>-->
                <div class="box-body">
                    @Html.Action("PageContent", new { Page = Page })
                </div>
            </div>
        </div>
    </div>
</section>