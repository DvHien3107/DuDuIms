@section script{
    <script src="~/Scripts/upload_and_review.js"></script>



    <script type="text/javascript">
        $(function () {

            $.ajax({
                method: "POST",
                url: "@Url.Action("LoadListState", "Merchantman", new {area = "" })",//"Merchantman/LoadListState",
                dataType: "json"
            })
                .done(function (data) {
                    //alert("ok");
                    //console.log(data);
                    $(".SalonState").autocomplete({
                        source: data[0],
                        minLength: 0
                    }).focus(function () {
                        $(this).autocomplete('search', $(this).val())
                    });

                });




            var _label = $("#morefile").find("label").not(".custom-file-upload");
            //console.log(_label);
            for (i = 0; i < _label.length; i++) {
                _label[i].remove();
            }
        });

        function readURL(input) {
            if (input.files && input.files[0]) {
                var inputid = $(input).attr("id");
                var reader = new FileReader();

                reader.onload = function (e) {
                    //$('#blah').attr('src', e.target.result);
                    var content = '<br/><img id="img_' + inputid + '" src="' + e.target.result + '" height="150"/><br>';
                    $(content).insertBefore("#btn-add-more");
                }

                reader.readAsDataURL(input.files[0]);
            }
        }

        $("#snap1").change(function () {
            readURL(this);
        });
    </script>
}

