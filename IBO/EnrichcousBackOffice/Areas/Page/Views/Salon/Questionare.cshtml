            width: 250px;
        }

        .img-display img {
            object-fit: contain;
            display: block;
            max-width: 250px;
            max-height: 250px;
        }


        .custom-file-upload {
            margin-top: 15px;
            background: #2b97e6;
            color: white;
            padding: 3px 10px;
            font-size: 14px;
        }

            .custom-file-upload i {
                padding-right: 7px;
            }

        .view-name-img {
            width: 200px !important;
            height: 28px !important;
        }
        .SS4_Name, .SS4_Name:focus, .SS4_Name:hover {
            border: none;
            outline:none;
            box-shadow:none;
        }
    </style>
